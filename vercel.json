{
  "builds": [
    {
      "src": "server/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "pubspec.yaml",
      "use": "@vercel/flutter",
      "config": {
        "outputDirectory": "build/web"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "build/web/index.html"
    }
  ]
}